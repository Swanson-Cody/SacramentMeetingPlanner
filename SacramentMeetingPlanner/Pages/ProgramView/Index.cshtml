@page
@model SacramentMeetingPlanner.Pages.Program.IndexModel

@{
    ViewData["Title"] = "Programs";
}

<h1>Programs</h1>

<p>
    <a asp-page="Create">Create New Program</a>
</p>

@foreach (var item in Model.Program)
{
    <h5>
        <a asp-page="DisplayProgram" asp-route-id="@item.ProgramID">@Html.DisplayFor(Model => item.DateOfMeeting)<a asp-page="./Edit" asp-route-id="@item.ProgramID">Edit</a> <span>|</span> <a asp-page="./Delete" asp-route-id="@item.ProgramID">Delete</a></a>
    </h5>
}